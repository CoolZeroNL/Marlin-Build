function fix_old_stuff() {
  # fix 404 for: TMC26XStepper (The library is included in Marlin\src\module\stepper)
  sed -i '/https:\/\/github.com\/trinamic\/TMC26XStepper\/archive\/master.zip/d' platformio.ini
}